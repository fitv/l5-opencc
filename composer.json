{
    "name": "mvmx/l5-opencc",
    "description": "OpenCC for Laravel",
    "keywords": [
        "opencc",
        "laravel"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mvmx/l5-opencc/issues",
        "source": "https://github.com/mvmx/l5-opencc"
    },
    "authors": [
        {
            "name": "MX Ma",
            "email": "mx.ma@qq.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Mvmx\\OpenCC\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvmx\\OpenCC\\ServiceProvider"
            ],
            "aliases": {
                "OpenCC": "Mvmx\\OpenCC\\Facade"
            }
        }
    }
}
